    "tslint": "5.8.0",
    "tslint-loader": "3.5.3",
    "typescript": "2.5.3",
    "underscore": "1.8.3",
    "url-loader": "0.5.7",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.3"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.1",
