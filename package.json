    "html-webpack-plugin": "2.30.1",
    "immutable": "3.8.2",
    "json-loader": "0.5.7",
    "moment": "2.19.1",
    "object-assign": "4.1.1",
    "react": "16.0.0",
    "react-bootstrap": "0.31.5",
    "react-datetime": "2.10.3",
    "react-di": "0.3.1",
    "react-fontawesome": "1.6.1",
    "react-hot-loader": "3.1.1",
    "react-markdown": "2.5.0",
    "react-markdown-editor": "0.6.2",
    "react-motion": "0.5.2",
    "react-redux": "5.0.6",
    "react-redux-form": "1.15.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "react-shallow-testutils": "2.0.0",
