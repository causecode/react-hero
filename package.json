{
  "name": "react-hero",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "public/index.js",
  "typings": "public/index.d.ts",
  "scripts": {
    "start": "npm run clean && webpack-dev-server --production",
    "build": "npm run clean && webpack",
    "clean": "rimraf public dist",
    "compile": "npm run clean && tsc",
    "postInstall": "npm run compile",
    "test": "NODE_ENV=test jest --no-cache",
    "lint-ts": "tslint 'src/*.tsx' 'src/**/*.tsx' 'src/*.ts' 'src/**/*.ts'",
    "start:dev": "npm run clean && webpack-dev-server --watch --hot",
    "buildCLI": "npm run compile",
    "linkCLI": "npm run buildCLI && npm link"
  },
  "bin": {
    "RH-generate-edit": "public/cli/generateEditPage.js",
    "RH-generate-create": "public/cli/generateCreatePage.js",
    "RH-generate-show": "public/cli/generateShowPage.js",
    "RH-generate-list": "public/cli/generateListPage.js",
    "RH-generate-all": "public/cli/generateAll.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/causecode/react-hero.git"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$"
  },
