{
  "name": "react-hero",
  "version": "2.0.2",
  "description": "",
  "private": true,
  "main": "public/index.js",
  "typings": "public/index.d.ts",
  "scripts": {
    "start": "npm run clean && webpack-dev-server",
    "build": "npm run clean && webpack",
    "clean": "rimraf public dist",
    "compile": "npm run clean && tsc",
    "postInstall": "npm run compile",
    "test": "NODE_ENV=test jest --no-cache --coverage",
    "lint-ts": "tslint 'src/*.tsx' 'src/**/*.tsx' 'src/*.ts' 'src/**/*.ts'",
    "start:dev": "npm run clean && webpack-dev-server --watch --hot --inline",
    "buildCLI": "npm run compile",
    "linkCLI": "npm run buildCLI && npm link"
  },
  "bin": {
    "RH-generate-create": "public/cli/getCreatePage.js",
    "RH-generate-edit": "public/cli/getEditPage.js",
    "RH-generate-show": "public/cli/getShowPage.js",
    "RH-generate-list": "public/cli/getListPage.js",
    "RH-generate-all": "public/cli/generateAll.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/causecode/react-hero.git"
  },
  "jest": {
    "verbose": true,
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "setupTestFrameworkScriptFile": "<rootDir>/__mocks__/localStorage.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/causecode/react-hero#readme",
