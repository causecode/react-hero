import * as React from 'react';
import {Table} from 'react-bootstrap';
import {ModelPropTypes, IInstancePageProps, DefaultModel} from 'react-hero';

export class <%= modelName.capitalize()%>ShowPage extends React.Component<IInstancePageProps, void> {

    static resourceName: string = '<%= resourceName%>';
    
    static defaultProps: IInstancePageProps = {
        instance: new DefaultModel({})
    };

    render(): JSX.Element {
        const {instance} =  this.props;
        return (
            <Table responsive bordered className="<%= resourceName%>-show-page">
                <thead>
                    <tr>
                        <th>Property</th>
                        <th>Value</th>
                    </tr>
                </thead>
                <tbody>
                    <% _.keys(tableRowMap).forEach(function (key) { %>
                    <%= tableRowMap[key]%>
                    <% }) %>
                </tbody>
            </Table>
        );
    }
}
